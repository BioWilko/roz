{
    "version": "1",
    "pathogen_configs": [
        "mpx",
        "mscape"
    ],
    "configs": {
        "mpx": {
            "file_naming": {
                "identical": true,
                "allowed_characters": "alphanumeric,_,-,."
            },
            "files": [
                "fasta",
                "csv",
                "bam"
            ],
            "fasta": {
                "header_allowed_characters": "alphanumeric,_,-, ,.",
                "iupac_only": true,
                "min_length": 100000
            },
            "bam": {
                "allowed_refs": [
                    "ON563414.3",
                    "MT903344.1"
                ],
                "compound_ref_pathogen_name": "MT903344.1",
                "human_ref_name": "GRCh38.p14",
                "check_dehumanised": true,
                "max_acceptible_human": 0.0001,
                "mapping_presets": {
                    "ILLUMINA": "sr",
                    "OXFORD_NANOPORE": "map-ont",
                    "PACIFIC_BIOSCIENCES": "map-pb",
                    "ION_TORRENT": "map-ont"
                },
                "require_sorted": true,
                "require_primertrimmed": false,
                "size_limit_gb": 2,
                "require_samtools_quickcheck": true
            },
            "csv": {
                "disallow_sample_id_elsewhere": true,
                "current_version": 1,
                "sample_name_allowed_characters": "alphanumeric,_,-",
                "required_fields": [
                    "pathogen_code",
                    "csv_template_version",
                    "site",
                    "sample_id",
                    "received_month",
                    "sample_type",
                    "seq_platform",
                    "instrument_model",
                    "enrichment_method",
                    "seq_strategy",
                    "source_of_library",
                    "run_name",
                    "bioinfo_pipe_name",
                    "bioinfo_pipe_version",
                    "country",
                    "run_layout",
                    "ukhsa_region"
                ],
                "optional_fields": [
                    "collection_month",
                    "patient_ageband",
                    "travel_status",
                    "outer_postcode",
                    "epi_cluster",
                    "patient_id",
                    "sample_site"
                ],
                "field_datatypes": {
                    "pathogen_code": "text",
                    "csv_template_version": "integer",
                    "site": "choice",
                    "sample_id": "text",
                    "received_month": "month",
                    "collection_month": "month",
                    "sample_type": "choice",
                    "sample_site": "choice",
                    "source_of_library": "choice",
                    "seq_strategy": "choice",
                    "seq_platform": "choice",
                    "enrichment_method": "choice",
                    "run_layout": "choice",
                    "run_name": "text",
                    "instrument_model": "text",
                    "bioinfo_pipe_name": "text",
                    "bioinfo_pipe_version": "text",
                    "patient_ageband": "choice",
                    "country": "choice",
                    "ukhsa_region": "choice",
                    "outer_postcode": "text",
                    "patient_id": "text",
                    "epi_cluster": "text",
                    "travel_status": "choice"
                },
                "character_limits": {
                    "site": "1-8",
                    "sample_id": "8-24",
                    "run_name": "18-96",
                    "instrument_model": "1-48",
                    "bioinfo_pipe_name": "1-96",
                    "bioinfo_pipe_version": "1-48",
                    "outer_postcode": "3-5",
                    "patient_id": "5-24",
                    "epi_cluster": "5-24"
                },
                "field_choices": {
                    "site": [
                        "BIRM"
                    ],
                    "run_layout": [
                        "SINGLE",
                        "PAIRED"
                    ],
                    "enrichment_method": [
                        "OTHER",
                        "PCR",
                        "RANDOM",
                        "RANDOM_PCR",
                        "NONE"
                    ],
                    "source_of_library": [
                        "GENOMIC",
                        "METAGENOMIC",
                        "METATRANSCRIPTOMIC",
                        "OTHER",
                        "TRANSCRIPTOMIC",
                        "VIRAL_RNA"
                    ],
                    "seq_strategy": [
                        "AMPLICON",
                        "OTHER",
                        "TARGETED_CAPTURE",
                        "WGA",
                        "WGS"
                    ],
                    "seq_platform": [
                        "ILLUMINA",
                        "OXFORD_NANOPORE",
                        "PACIFIC_BIOSCIENCES",
                        "ION_TORRENT"
                    ],
                    "sample_type": [
                        "SWAB",
                        "SERUM"
                    ],
                    "patient_ageband": [
                        "0-5",
                        "6-10",
                        "11-15",
                        "16-20",
                        "21-25",
                        "26-30",
                        "31-35",
                        "36-40",
                        "41-45",
                        "46-50",
                        "51-55",
                        "56-60",
                        "61-65",
                        "66-70",
                        "71-75",
                        "76-80",
                        "81-85",
                        "86-90",
                        "91-95",
                        "96-100",
                        "100+"
                    ],
                    "country": [
                        "ENG",
                        "WALES",
                        "SCOT",
                        "NI"
                    ],
                    "travel_status": [
                        "YES",
                        "NO"
                    ],
                    "ukhsa_region": [
                        "NE"
                    ],
                    "sample_site": [
                        "SORE",
                        "GENITAL"
                    ]
                }
            }
        },
        "mscape": {
            "file_naming": {
                "identical": true,
                "allowed_characters": "alphanumeric,_,-,."
            },
            "artifact_layout": "project.sample_name.run_name",
            "files": [
                ".1.fastq.gz",
                ".2.fastq.gz",
                ".csv"
            ],
            "file_specs": {
                "illumina": {
                    ".1.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".2.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".csv": {
                        "sections": 4,
                        "layout": "project.sample_id.run_name.ftype"
                    },
                    "match_size": 3,
                    "files": [
                        ".1.fastq.gz",
                        ".2.fastq.gz",
                        ".csv"
                    ]
                },
                "ont": {
                    ".fastq.gz": {
                        "sections": 5,
                        "layout": "project.sample_id.run_name.ftype.gzip"
                    },
                    ".csv": {
                        "sections": 4,
                        "layout": "project.sample_id.run_name.ftype"
                    },
                    "match_size": 2,
                    "files": [
                        ".ont.fastq.gz",
                        ".csv"
                    ]
                }
            }
        },
        "mscapetest": {
            "file_naming": {
                "identical": true,
                "allowed_characters": "alphanumeric,_,-,."
            },
            "artifact_layout": "project.sample_id.run_name",
            "files": [
                ".1.fastq.gz",
                ".2.fastq.gz",
                ".csv"
            ],
            "file_specs": {
                "illumina": {
                    ".1.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".2.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".csv": {
                        "sections": 5,
                        "layout": "project.sample_id.run_name.platform.ftype"
                    },
                    "match_size": 3,
                    "files": [
                        ".1.fastq.gz",
                        ".2.fastq.gz",
                        ".csv"
                    ]
                },
                "ont": {
                    ".fastq.gz": {
                        "sections": 5,
                        "layout": "project.sample_id.run_name.ftype.gzip"
                    },
                    ".csv": {
                        "sections": 5,
                        "layout": "project.sample_id.run_name.platform.ftype"
                    },
                    "match_size": 2,
                    "files": [
                        ".fastq.gz",
                        ".csv"
                    ]
                }
            }
        },
        "pathsafe": {
            "file_naming": {
                "identical": true,
                "allowed_characters": "alphanumeric,_,-,."
            },
            "artifact_layout": "project.sample_id.run_name",
            "files": [
                ".1.fastq.gz",
                ".2.fastq.gz",
                ".csv"
            ],
            "file_specs": {
                "illumina": {
                    ".1.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".2.fastq.gz": {
                        "sections": 6,
                        "layout": "project.sample_id.run_name.direction.ftype.gzip"
                    },
                    ".csv": {
                        "sections": 5,
                        "layout": "project.sample_id.run_name.platform.ftype"
                    },
                    "match_size": 3,
                    "files": [
                        ".1.fastq.gz",
                        ".2.fastq.gz",
                        ".csv"
                    ]
                }
            }
        }
    }
}